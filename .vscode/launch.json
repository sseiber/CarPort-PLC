{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug buildTypeSchemas.js",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "build:schemas"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Carport PLC",
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build-carport-plc",
            "program": "${workspaceFolder}/src/index.ts",
            "console": "integratedTerminal",
            "smartStep": true,
            "showAsyncStacks": true,
            "sourceMaps": true,
            "env": {
                "NODE_ENV": "local",
                "CARPORT_PLC_STORAGE": "storage"
            },
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "presentation": {
                "group": "ts"
            }
        }
    ]
}